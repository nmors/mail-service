{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch App",
            "program": "${workspaceFolder}\\src\\main.ts",
            "outFiles": [
                "${workspaceFolder}\\**\\*.ts"
            ]
        }, {
            "name": "Debug Jest Tests",
            "type": "node",
            "request": "launch",
            "runtimeArgs": [
                "--inspect-brk",
                "${workspaceRoot}\node_modules\\jest\\bin\\jest.js",
                "--runInBand"
            ],
           
        }
    ]
}